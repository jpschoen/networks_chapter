X<-c(0,1,0,1,0,1,1,1,1,1) # No obs. where Y=1 and X=0
logL <- function(param) {
b0<-param[1]
b1<-param[2]
ll<-Y*log(exp(b0+b1*X)/(1+exp(b0+b1*X))) +
(1-Y)*log(1-(exp(b0+b1*X)/(1+exp(b0+b1*X)))) # lnL for binary logit
ll
} # Logit regression function
